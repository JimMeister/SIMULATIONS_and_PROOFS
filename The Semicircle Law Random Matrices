plot(x=NULL,y=NULL,xlim= c(-0.3,0.3),
     ylim=c(0,3.5),main=
         "The Semicircle Law",
     xlab="Eigenvalue Random Matrix",
     ylab="Density")
for(i in 1:100){
    set.seed(i)
    n <- 10000
    M <- matrix(rnorm(n),nrow=100)
    M <- 1/sqrt(2*n)*(M+t(M))
    eval <- eigen(M)$values
    d <- density(eval)
    lines(d,col = i, lwd = 2)
}
